using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class Enviroment : MonoBehaviour
{
    [SerializeField]
    private float heal;

    private float maxHeal;
    void Start()
    {
        
    }

    // Update is called once per frame
    void Update()
    {
        if (heal <= 0 )
        {
            Destroy(gameObject);
        }
    }
}
