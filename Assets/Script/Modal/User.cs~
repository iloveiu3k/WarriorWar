using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class User : MonoBehaviour
{
    // Start is called before the first frame update
    public string nameUser { get; set; }
    public string avatarUser { get; set; }

    public string levelNumberUser { get; set; }

    public string levelBarUser { get; set; }

    public User(string nameUser,string avatarUser,string levelNumberUser, string levelBarUser)
    {
        this.nameUser = nameUser;
        this.avatarUser = avatarUser;
        this.levelNumberUser = levelNumberUser;
        this.levelBarUser = levelBarUser;
    }
}
